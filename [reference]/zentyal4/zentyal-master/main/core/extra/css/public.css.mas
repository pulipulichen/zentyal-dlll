.final_installation .btn {
    position: absolute;
    bottom: 50px;
}
.first-install #nav.install-steps {
    overflow: hidden;
}
#nav.install-steps li {
    background-position: 20px center;
    background-repeat: no-repeat;
    border-top: 0;
