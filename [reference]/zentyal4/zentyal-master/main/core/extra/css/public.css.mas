    margin-top: 5px;
    font-size: 1.2em;
}
.final_installation {
    height:450px;
    background: url("/data/software/images/fluffy_ok.png") right bottom no-repeat;
    background-color: #ddd;
    margin-top:10px;
    overflow: hidden;
    position: relative;
    padding: 40px 50px;
}
.final_installation h4 {
    font-size: 50px;
    font-weight: bold;
    color: #666;
}
.final_installation h5 {
    font-size: 20px;
    color: #666;
    margin: 15px 0px;
    width: 40%;
