#nav.install-steps li:last-child {
    border-right: 1px solid #cccccc;
    border-bottom: 0;
}
#nav.install-steps .step-done {
    background-image: url(/data/images/apply-g.gif);
}
#nav.install-steps .step-actual{
    background-image: url(/data/images/f.gif);
    font-weight: bold;
