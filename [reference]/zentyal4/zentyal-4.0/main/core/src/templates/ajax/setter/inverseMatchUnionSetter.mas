<%args>
$tableName
$data
$cssClass => ''
</%args>
% my $id = $tableName . '_' . $data->fieldName() . '_inverseMatch';
			<& '/ajax/setter/unionSetter.mas', 'data' => $data, 'tableName' => $tableName, 'cssClass' => $cssClass &>
            
            <label class="checkbox">
			<& /input/checkbox.mas,
			  'name'  => "$id", 
			  'value' => $data->inverseMatch(),
			  'id'    => "$id",
			  'class' => "$cssClass",
			&>
			<% __('Inverse match')%>
			</label>